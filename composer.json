{
  "name": "rebilly/country",
  "description": "country",
  "license": "MIT",
  "keywords": ["country"],
  "homepage": "https://github.com/rebilly/country/",
  "authors": [
    {
      "name": "Rebilly",
      "homepage": "https://github.com/rebilly"
    }
  ],
  "minimum-stability": "stable",

  "require": {
    "php": ">=7.1",
    "ext-json": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.8",
    "phpunit/phpunit": "^9.3",
    "php-coveralls/php-coveralls": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Country\\": ["src"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Country\\": ["tests"]
    }
  },
  "scripts": {
    "cs": "php-cs-fixer fix --verbose --dry-run --ansi",
    "cs-fix": "php-cs-fixer fix --verbose --ansi",
    "test": "phpunit"
  }
}
